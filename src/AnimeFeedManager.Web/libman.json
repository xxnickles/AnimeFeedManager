  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "htmx@1.9.11",
      "destination": "wwwroot/lib/htmx/",
      "files": [
        "htmx.min.js",
        "htmx.js",
        "ext/ws.min.js",
        "ext/sse.min.js"
      ]
    },
    {
      "library": "alpinejs@3.13.8",
      "destination": "wwwroot/lib/alpinejs/",
      "files": [
        "cdn.js",
        "cdn.min.js",
        "module.cjs.js",
        "module.cjs.min.js",
        "module.esm.js",
        "module.esm.min.js"
