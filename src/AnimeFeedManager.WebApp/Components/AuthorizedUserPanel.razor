@inject NavigationManager NavigationManager
<MudFab Color="Color.Secondary" StartIcon="@Icons.Filled.Logout" Size="Size.Small" Label="Logout" OnClick="Logout"/>

@code {
    private string _logOutPath = "/.auth/logout?post_logout_redirect_uri=/";

    private void Logout()
    {
        NavigationManager.NavigateTo(_logOutPath, true);
    }

}