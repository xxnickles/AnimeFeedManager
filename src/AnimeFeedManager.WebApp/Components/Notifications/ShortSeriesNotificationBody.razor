@using AnimeFeedManager.Common.Notifications
<MudText Typo="Typo.body1">Notification for the following series was sent at @Notification.Time.ToString()</MudText>
@foreach (var feed in Notification.Feeds)
{
    <MudText Typo="Typo.body1">@feed.Title</MudText>
}

@code {

    [Parameter]
    public ShortSeriesNotification Notification { get; set; }

}