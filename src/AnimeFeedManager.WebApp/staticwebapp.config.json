{
  "routes": [
    {
      "route": "/login/github",
      "redirect": "/.auth/login/github"
    },
    {
      "route": "/login/twitter",
      "redirect": "/.auth/login/twitter"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/api/user*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/interested*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/subscriptions*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/removeInterested",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/unsubscribe",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ]
}